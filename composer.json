{
    "name": "sebastiaanluca/php-stub-generator",
    "type": "library",
    "description": "Easily generate files and classes from stubs.",
    "keywords": [
        "sebastiaanluca",
        "php",
        "stub",
        "file",
        "generator"
    ],
    "homepage": "https://github.com/sebastiaanluca/php-stub-generator",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Luca",
            "email": "hello@sebastiaanluca.com",
            "homepage": "https://www.sebastiaanluca.com",
            "role": "Author"
        }
    ],
    "require": {
        "php": "^7",
        "illuminate/support": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6",
        "raveren/kint": "^1"
    },
    "autoload": {
        "psr-4": {
            "SebastiaanLuca\\StubGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SebastiaanLuca\\StubGenerator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
